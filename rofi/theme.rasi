* {
    background-color: transparent;
    text-color: #FFFFFF;
    font: "Fira Code 12";
}

window {
    background-color: transparent;
    border: 2px;
    border-radius: 10px;
    padding: 10px;
}

inputbar {
    background-color: rgba(0, 0, 0, 0.4);
    border: 1px;
    padding: 10px;
}

listview {
    background-color: rgba(0, 0, 0, 0.2);
    padding: 10px;
}

element {
    padding: 5px;
    border-radius: 5px;
}

element selected {
    background-color: rgba(0, 0, 0, 0.5);
}

